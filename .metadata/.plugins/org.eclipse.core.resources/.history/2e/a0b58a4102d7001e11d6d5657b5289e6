package com.kafkaproducer.serviceimpl;

import org.slf4j.Logger;
import org.slf4j.LoggerFactory;
import org.springframework.stereotype.Service;

import com.kafkaproducer.service.MessageDrivenService;

/**
*
* @author Ratnesh
* @DateTime 29-Feb-2024 3:32:10 pm
*
**/
@Service
public class MessageDrivenServiceImpl implements MessageDrivenService {

	public static final Logger LOGGER = LoggerFactory.getLogger(MessageDrivenServiceImpl.class);
	
	
	@Override
	public void produceMessage(String Message) {
		// TODO Auto-generated method stub
		LOGGER.info(LOGGER.toString());
		LOGGER.info("Produced Message :"+Message);
		
	}

}
